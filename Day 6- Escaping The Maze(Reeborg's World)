#The Hurdle Problem
def hurdle():                #Repeat this 6 times
    turn_left()
    move()
    turn_left()
    turn_left()
    turn_left()

#For Random Hurdles (using loop)

def hurdle():    
    turn_left()
    move()
    turn_left()
    turn_left()
    turn_left()
    move()
    turn_left()
    turn_left()
    turn_left()
    move()
    turn_left()
    turn_left()
    turn_left()
    turn_left()
    turn_left()
    
while not at_goal():
    if wall_in_front():
        hurdle()
    else:
        move()







